{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["es2018", "dom", "esnext", "DOM.Iterable"],
        "moduleResolution": "node",
        "jsx": "react",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitOverride": true,
        "noUncheckedIndexedAccess": true,
        "useUnknownInCatchVariables": false,
        "noEmit": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "declaration": true
    },
    "include": ["boxes"]
}
